{
	"schemaVersion": 1,
	"id": "battlefactions",
	"version": "${version}",
	"name": "Example mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Gabiru",
		"RogerGay"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/battlefactions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.battlefactions.BattleFactionsMod"
		],
		"client": [
			"com.battlefactions.BattleFactionsModClient"
		],
        "fabric-datagen": [
            "com.battlefactions.docs.datagen.BattleFactionsModDataGenerator"
        ]
    },
	"mixins": [
		"battlefactions.mixins.json",
		{
			"config": "battlefactions.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}